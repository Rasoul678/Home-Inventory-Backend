{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "State",
    "description": "The State",
    "required": [
        "country_id",
        "name"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database Id of the state"
        },
        "country_id": {
            "$id": "#/properties/country_id",
            "type": "integer",
            "title": "The database Id of the country"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the state"
        },
        "code": {
            "$id": "#/properties/code",
            "type": "string",
            "title": "The code of the state"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The created_at schema"
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The updated_at schema"
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "boolean",
            "title": "The deleted_at schema"
        }
    },
    "additionalProperties": false
}