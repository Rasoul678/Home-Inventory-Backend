{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Country",
    "description": "The Country",
    "required": [
        "name"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database Id of the country"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the country"
        },
        "code": {
            "$id": "#/properties/code",
            "type": "string",
            "title": "The code of the country"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The created_at schema"
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The updated_at schema"
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "boolean",
            "title": "The deleted_at schema"
        }
    },
    "additionalProperties": false
}