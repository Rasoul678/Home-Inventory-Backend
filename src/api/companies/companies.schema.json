{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Company",
    "description": "The Company",
    "required": [
        "address_id",
        "name",
        "type"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database Id of the company"
        },
        "address_id": {
            "$id": "#/properties/address_id",
            "type": "integer",
            "title": "The id of the address"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the company"
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description of the company"
        },
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type of the company"
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email of the company"
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo url of the company"
        },
        "website_url": {
            "$id": "#/properties/website_url",
            "type": "string",
            "title": "The website url of the company"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The created_at schema"
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The updated_at schema"
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "boolean",
            "title": "The deleted_at schema"
        }
    },
    "additionalProperties": false
}