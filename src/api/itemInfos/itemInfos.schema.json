        },
        "purchase_date": {
            "$id": "#/properties/purchase_date",
            "type": "string",
            "title": "The purchase date"
        },
        "expiration_date": {
            "$id": "#/properties/expiration_date",
            "type": "string",
            "title": "The expiration date"
        },
        "purchase_price": {
            "$id": "#/properties/purchase_price",
